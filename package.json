{
  "name": "honey-badger-testing",
  "version": "1.0.0",
  "description": "A collection of scripts to test the Honey Badger BFT integration in Parity",
  "main": "src/index.js",
  "scripts": {
    "tsc": "tsc",
    "test": "mocha",
    "keepalive-wait": "node src/keepalive-wait.js",
    "keepalive-nowait": "node src/keepalive-nowait.js",
    "goLive": "npm run tsc && node dist/thomas_playground.js",
    "continous": "npm run tsc && node dist/runContinousTransactionsSender.js",
    "pendingTransactions": "npm run tsc && node dist/runShowPendingTransactions.js",
    "onceASecond1": "NODE_ENV=onceASecond1 node dist/runContinousTransactionsSender.js",
    "onceASecond2": "NODE_ENV=onceASecond2 node dist/runContinousTransactionsSender.js",
    "testboth": "npm-run-all --parallel onceASecond1 onceASecond2"
  },
  "repository": {
    "type": "git",
    "url": "git@code.lab10.io:graz/04-artis/honey-badger-testing.git"
  },
  "keywords": [
    "bootstrap",
    "linting",
    "testing"
  ],
  "author": "dfo@lab10.coop",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@types/underscore": "^1.9.4",
    "bignumber.js": "^9.0.0",
    "bip39": "^3.0.2",
    "config": "^3.3.0",
    "csv-writer": "^1.6.0",
    "ethereumjs-wallet": "^0.6.3",
    "underscore": "^1.9.2",
    "web3": "^1.2.6",
    "websocket": "^1.0.31"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-utils": ">=1.4.1",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.8.3"
  }
}
